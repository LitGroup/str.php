{
    "name": "litgroup/str",
    "description": "String utils for PHP.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Roman Shamritskiy",
            "email": "roman@litgroup.ru"
        }
    ],
    "autoload": {
        "psr-4": {
            "LitGroup\\Str\\": "src/"
        }
    },
    "scripts": {
        "test": [
            "vendor/bin/phpunit --configuration ."
        ],
        "test-coverage-html": [
            "phpdbg -qrr vendor/bin/phpunit --configuration . --coverage-html build/coverage"
        ],
        "test-ci": [
            "phpdbg -qrr vendor/bin/phpunit --configuration . --coverage-text --colors never"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Test\\LitGroup\\Str\\": "tests/"
        }
    },
    "require": {},
    "require-dev": {
        "phpunit/phpunit": "^6.3"
    }
}
